@Article{VandenBroeck2012,
  Title                    = {{Molecular markers associated with outcome and metastasis in human pancreatic cancer}},
  Author                   = {{Van den Broeck}, Anke and Vankelecom, Hugo and {Van Eijsden}, Rudy and Govaere, Olivier and Topal, Baki},
  Journal                  = {Journal of Experimental \& Clinical Cancer Research},
